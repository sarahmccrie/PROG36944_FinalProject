  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>FinalProjectERMS_DataSource</DataSourceName>
        <CommandText>SELECT
  Employees.Name AS [Employees Name]
  ,Employees.Email
  ,TaskItems.TaskId
  ,TaskItems.Name AS [TaskItems Name]
  ,TaskItems.Priority
  ,TaskItems.Status
FROM
  Employees
  INNER JOIN TaskItems
    ON Employees.EmployeeId = TaskItems.AssignedEmployeeId</CommandText>
        <rd:DesignerState>
          <QueryDefinition xmlns="http://schemas.microsoft.com/ReportingServices/QueryDefinition/Relational">
            <AutoJoin>False</AutoJoin>
