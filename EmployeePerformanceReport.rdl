              <ColumnExpression ColumnOwner="TaskItems" ColumnName="TaskId" />
              <ColumnExpression ColumnOwner="TaskItems" ColumnName="Name" />
              <ColumnExpression ColumnOwner="TaskItems" ColumnName="Priority" />
              <ColumnExpression ColumnOwner="TaskItems" ColumnName="Status" />
            </SelectedColumns>
            <Joins>
              <Join Type="CROSS JOIN" ColumnOwner="Employees" />
              <Join Type="INNER JOIN" ColumnOwner="TaskItems">
                <On>
                  <From ColumnOwner="Employees" ColumnName="EmployeeId" />
                  <To ColumnOwner="TaskItems" ColumnName="AssignedEmployeeId" />
                </On>
              </Join>
            </Joins>
          </QueryDefinition>
        </rd:DesignerState>
      </Query>
